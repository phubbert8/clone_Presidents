# Deploy plumber API to digital ocean droplet

# packages ---
library(plumber)
library(analogsea)

# Provision DO droplet
plumber_do <- do_provision(name ="plumber", region = "sfo2")

# Publish API to droplet
do_deploy_api(droplet = plumber_do,
              path = " pres_plumber",
              localPath = here::here("R", " pres_plumber"),
              port = 8028,
              forward = TRUE,
              swagger = TRUE)
